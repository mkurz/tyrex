<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE jpl SYSTEM "jpl.dtd" >

<jpl version="1.5">
	<program type="application">
		<application
			args="./dist/tyrex-0.9.8.7.jar"
			working_dir="tyrex"
			source_dir="./src/main:./src/tests/unit"
			classname="tyrex.Unit">
			<classpath>
				<classpath.archive
					location="./lib/Folio-0.3.jar"
					type="jar"/>
				<classpath.archive
					location="./lib/ant_1.3.jar"
					type="jar"/>
				<classpath.archive
					location="./lib/ant_optional_1.3.jar"
					type="jar"/>
				<classpath.archive
					location="./lib/castor-0.9.2.jar"
					type="jar"/>
				<classpath.archive
					location="./lib/fop_0.14.jar"
					type="jar"/>
				<classpath.archive
					location="./lib/intalio_build_0.5.jar"
					type="jar"/>
				<classpath.archive
					location="./lib/jaas_1.0.jar"
					type="jar"/>
				<classpath.archive
					location="./lib/javamail_1.2.jar"
					type="jar"/>
				<classpath.archive
					location="./lib/jca_1.0.jar"
					type="jar"/>
				<classpath.archive
					location="./lib/jdbc2_0-stdext.jar"
					type="jar"/>
				<classpath.archive
					location="./lib/jta_1.0.1.jar"
					type="jar"/>
				<classpath.archive
					location="./lib/junit_3.5.jar"
					type="jar"/>
				<classpath.archive
					location="./lib/ldapjdk_4.1.jar"
					type="jar"/>
				<classpath.archive
					location="./lib/log4J_1.0.4.jar"
					type="jar"/>
				<classpath.archive
					location="./lib/ots-jts_1.0.jar"
					type="jar"/>
				<classpath.archive
					location="./lib/w3c.jar"
					type="jar"/>
				<classpath.archive
					location="./lib/xerces-J_1.4.0.jar"
					type="jar"/>
				<classpath.archive
					location="./lib/xslp_1.1.jar"
					type="jar"/>
				<classpath.archive
					location="./dist/tyrex-0.9.8.7.jar"
					type="jar"/>
				<classpath.path location=".src/etc"/>
				<classpath.path location="./build/tests"/>
				<classpath.path location="./build/tests/unit"/>
			</classpath>
		</application>
		<applet
			working_dir=""
			source_dir=""
			htmlfile=""
			main_package="">
			<classpath/>
		</applet>
		<serverside
			suggested_filters=""
			id=""
			server_dir=""
			prepend_to_vm_args=""
			source_dir=""
			classname=""
			main_package=""
			exclude_server_classes="true"
			args=""
			working_dir=""
			prepend_to_classpath="">
			<classpath/>
		</serverside>
	</program>
	<vm
		snapshot_dir="/tmp"
		location="/usr/java/jdk1.3.1/bin/java"
		args=""
		type="java2"
		use_jit="true"/>
	<viewer
		socket=":4444"
		type="local"/>
	<analysis type="coverage">
		<performance
			record_from_start="true"
			timing="elapsed"
			track_natives="false"
			final_snapshot="true"
			granularity="method">
			<performance.filter
				visibility="invisible"
				methodmask="*"
				enabled="true"
				classmask="*"
				time="track"
				granularity="method"/>
			<performance.filter
				visibility="visible"
				methodmask="*"
				enabled="true"
				classmask="tyrex.*"
				time="track"
				granularity="method"/>
		</performance>
		<heap
			record_from_start="true"
			no_stack_trace_limit="false"
			final_snapshot="false"
			max_stack_trace="4"
			track_dead_objects="false"/>
		<threadalyzer
			record_from_start="true"
			write_to_console="false">
			<deadlock_detection
				enabled="true"
				deadlock_and_exit="true"
				report_stalls="false"
				track_system_threads="false"
				block_can_stall="false"
				deadlock_threshold="2"/>
			<deadlock_prediction
				enable_hold_and_wait="false"
				enable_lock_order="false"
				lock_order_maintains_covers="true"/>
			<data_race
				ignore_volatile="false"
				enable_happens_before="false"
				no_stack_trace_limit="false"
				enable_lock_covers="false"
				max_stack_trace="1"
				instrument_elements="false"/>
			<visualizer
				enabled="true"
				visualization_level="1"/>
			<threadalyzer.filter
				visibility="invisible"
				enabled="true"
				classmask="*"/>
			<threadalyzer.filter
				visibility="visible"
				enabled="true"
				classmask="tyrex.*"/>
		</threadalyzer>
		<coverage
			record_from_start="true"
			final_snapshot="true"
			granularity="line">
			<coverage.filter
				visibility="invisible"
				methodmask="*"
				enabled="true"
				classmask="*"/>
			<coverage.filter
				visibility="visible"
				methodmask="*"
				enabled="true"
				classmask="tyrex.*"/>
		</coverage>
	</analysis>
</jpl>
